   <recordinfo xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:my="http://nowhere"
      xmlns:marc="http://www.loc.gov/MARC21/slim" detail="brief">
      <str name="Author">Otto III, Holy Roman Emperor, 980-1002.</str>
      <str name="Title">Diploma issued to Adam, son of Teutio, 1000.</str>
      <str name="Physical description">1 item (1 leaf) : parchment ; 36 x 44 cm.</str>
      <str name="Summary">Diploma issued to Adam, son of Teutio. It takes Adam under imperial protection and gives him
         the right to live under Roman law.</str>
      <str name="Notes">Dated 10 July 1000, in Pavia. Date given on line 11.</str>
      <str name="Notes">Written in a notarial hand.</str>
      <str name="Notes">Incipit: In nomine sanctae ... Notum sit omnibus fidelibus.</str>
      <str name="Notes">Explicit: ... hanc paginam sigillare iussimus. Heribertus cancellarius vice petri cumani
         episcopi novit.</str>
      <str name="Cite as">UPenn Ms. Oversize 8</str>
      <str name="Manuscript location">Henry Charles Lea Library, Special Collections, Van Pelt Library, University of
         Pennsylvania, Ms. Oversize 8</str>
   </recordinfo>
